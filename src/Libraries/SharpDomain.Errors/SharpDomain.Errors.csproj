<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <LangVersion>9.0</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <PropertyGroup>
        <PackageId>SharpDomain.Errors</PackageId>
        <Title>SharpDomain.Errors</Title>
        <Version>0.2.1</Version>
        <Authors>maszrum</Authors>
        <Company>SharpDomain</Company>
        <Product>SharpDomain</Product>
        <Description>Standard error classes for SharpDomain library</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/maszrum/SharpDomain</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>ddd, cqrs</PackageTags>
    </PropertyGroup>

</Project>
